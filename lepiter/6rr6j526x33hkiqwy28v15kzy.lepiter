								"emailString" : "<unknown>"
							},
							"createTime" : {
								"__type" : "time",
								"time" : {
									"__type" : "dateAndTime",
									"dateAndTimeString" : "2023-04-04T11:24:37.326841+02:00"
								}
							},
							"editEmail" : {
								"__type" : "email",
								"emailString" : "<unknown>"
							},
							"editTime" : {
								"__type" : "time",
								"time" : {
									"__type" : "dateAndTime",
									"dateAndTimeString" : "2023-04-04T11:24:48.657622+02:00"
								}
							},
							"uid" : {
								"__type" : "uid",
								"uidString" : "s9ky++C0DQCzZwkVANrPfQ=="
							},
							"paragraphStyle" : {
								"__type" : "textStyle"
							},
							"string" : "***OLD:*** We reimagine it from the ground up, and with it we reshape the whole development experience. We call it [[Moldable Development]]."
						},
						{
							"__type" : "textSnippet",
							"children" : {
								"__type" : "snippets",
								"items" : [ ]
							},
							"createEmail" : {
								"__type" : "email",
