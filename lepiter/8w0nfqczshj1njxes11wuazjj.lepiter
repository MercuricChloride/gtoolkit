							},
							"createTime" : {
								"__type" : "time",
								"time" : {
									"__type" : "dateAndTime",
									"dateAndTimeString" : "2021-06-06T21:31:41.187588+02:00"
								}
							},
							"editEmail" : {
								"__type" : "email",
								"emailString" : "<unknown>"
							},
							"editTime" : {
								"__type" : "time",
								"time" : {
									"__type" : "dateAndTime",
									"dateAndTimeString" : "2022-06-16T22:08:16.195141+02:00"
								}
							},
							"uid" : {
								"__type" : "uid",
								"uidString" : "a+0Otn+ADQCLLwn+AdDofA=="
							},
							"paragraphStyle" : {
								"__type" : "textStyle"
							},
							"string" : "Moldable Development is hard to convey in words. You have to experience it. We offer Glamorous Toolkit as free and open-source under the MIT license because we want you to experience it as directly as possible."
						},
						{
							"__type" : "textSnippet",
							"children" : {
